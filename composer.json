{
    "name": "neilmillard/slim3-skeleton",
    "description": "Simple Slim Framework 3 user management skeleton with Twig & Monolog, and Redbean.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
      {
        "name": "Neil Millard",
        "email": "neil@neilmillard.com"
      }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/neilmillard/authenticator"
    }
  ],
    "require": {
      "slim/slim": "^3.0",
      "slim/twig-view": "dev-master",
      "slim/flash": "dev-master",
      "monolog/monolog": "^1.13",
      "gabordemooij/redbean": "dev-master",
      "neilmillard/authenticator": "dev-master"
    },
    "minimum-stability": "dev"
}
